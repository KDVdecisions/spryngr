flattenDiscrete <- function(discreteSet){
observations <- c()
indices <- c()
#flatten all discrete data, save row indices to keep other obs
for(i in 1:NROW(discreteSet)){
for(ob in unlist(discreteSet[i])){
observations <- c(observations, ob)
indices <- c(indices, i)
}
}
print(data.frame(observations, indices))
return(data.frame(observations, indices))
}
